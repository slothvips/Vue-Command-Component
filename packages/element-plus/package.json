{
  "name": "@vue-cmd/element-plus",
  "version": "1.0.11",
  "type": "module",
  "description": "Element Plus命令式组件适配器 - 让Element Plus的Dialog、Drawer等组件支持命令式调用",
  "homepage": "https://slothvips.github.io/Vue-Command-Component/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/slothvips/Vue-Command-Component.git"
  },
  "keywords": [
    "vue",
    "vue3",
    "element-plus",
    "command",
    "component",
    "dialog",
    "drawer",
    "modal",
    "popup",
    "弹窗",
    "抽屉",
    "命令式",
    "imperative",
    "el-dialog",
    "el-drawer",
    "promise",
    "async",
    "adapter"
  ],
  "license": "MIT",
  "author": "slothvips",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "publish": "pnpm build && pnpm publish --access public --no-git-checks"
  },
  "dependencies": {},
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^4.0.0",
    "vue-tsc": "^2.0.0"
  },
  "peerDependencies": {
    "element-plus": "*",
    "vue": "*",
    "@vue-cmd/core": "workspace:*"
  },
  "publishConfig": {
    "access": "public"
  }
}
