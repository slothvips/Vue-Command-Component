{
  "name": "@vue-cmd/vant",
  "version": "1.1.3",
  "type": "module",
  "description": "Vant命令式组件适配器 - 让Vant的Popup、Dialog等组件支持命令式调用，专为移动端优化",
  "homepage": "https://slothvips.github.io/Vue-Command-Component/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/slothvips/Vue-Command-Component.git"
  },
  "keywords": [
    "vue",
    "vue3",
    "vant",
    "command",
    "component",
    "popup",
    "dialog",
    "mobile",
    "移动端",
    "弹窗",
    "对话框",
    "命令式",
    "imperative",
    "van-popup",
    "van-dialog",
    "promise",
    "async",
    "adapter",
    "h5"
  ],
  "license": "MIT",
  "author": "slothvips",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "publish": "pnpm build && pnpm publish --access public --no-git-checks"
  },
  "dependencies": {
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^4.0.0",
    "vue-tsc": "^2.0.0"
  },
  "peerDependencies": {
    "vant": "*",
    "vue": "*",
    "@vue-cmd/core": "workspace:*"
  }
}
